<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="CmmUse">

	<select id="CmmUseDAO.selectCmmCodeDetail" parameterClass="egovframework.com.cmm.ComDefaultCodeVO" resultClass="egovframework.rte.psl.dataaccess.util.EgovMap">
		<![CDATA[ 
			SELECT 
			CODE_ID
			, CODE
			, CODE_NM
			, CODE_DC
			FROM COMTCCMMNDETAILCODE
			WHERE USE_AT = 'Y'
		]]>
	</select>

</sqlMap>            